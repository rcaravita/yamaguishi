#cadastro
	%h2 Cadastro de Cliente

	= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f|
		= devise_error_messages!

		= render "admin/clients/form", :f => f
		
		.divider

		.right= f.submit "Cadastrar", class: "save"

		.left= link_to "JÃ¡ possui acesso? Login", new_session_path(resource_name), class: "button"
		.left= link_to "Esqueceu a senha?", new_password_path(resource_name), class: "button"